 <div id=<%=response.subscription._id%> class="col-md-10 col-md-offset-1">
          <div class="panel panel-info">
              <div id=<%=i%> class="panel-heading" style="padding: 5px; min-height: 50px;">
                <div id=<%=response.subscription.subscribed_user_id%> class="ad_options pull-right" style="margin-top:  3px; margin-right:8px;">
                  <a id="unsubscribePopUpButton" data-toggle="modal" 
                    data-target="#unsubscribePopUp<%=response.subscription.subscribed_user_id%>" class="btn btn-danger" >Unsubscribe</a>      
                </div>
                  <h4>
                  <strong>
                  <%=response.subscription.subscribed_user_name%> 
                  </strong> 
                  </h4>
              </div>
              <div class="panel-body" style="padding: 10px 15px 10px;">
                
                You subscribed to 
                <a href="/user?id=<%=response.subscription.subscribed_user_id%>">
                <%=response.subscription.subscribed_user_name%>
                </a> at 
                <span class="text-muted">
                <%=response.subscription.createdAt%>
                </span>.
              </div>
              <div class="panel-footer" align="center" style="padding: 5px;">
                <h4>
                  <strong>
                  </strong>
                </h4>
              </div>
              
          </div>
        </div>
<% include unsubscribe_popup%>
